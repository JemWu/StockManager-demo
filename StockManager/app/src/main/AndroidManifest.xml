<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.jem.stockmanager"

    >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.BLUETOOTH"></uses-permission>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH_PRIVILEGED"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

<activity android:name=".Settings"/>
        <activity android:name=".kinds_detail.brand_detail.brand_list" />
        <activity android:name=".kinds_detail.brand_detail.goods_detail.goods_list" />
        <activity android:name=".kinds_detail.brand_detail.goods_detail.goods_item_detail.goods_detail" />
        <activity android:name=".Sync" />
        <activity android:name=".Send" />
        <activity android:name=".Accept" />
    </application>

</manifest>